#import "RYGBaseParam.h"
#import "APService.h"

@implementation RYGBaseParam
- (id)init
{
    if (self = [super init]) {

        self.token = [RYGUtility getUserInfo].token;
        self.device = @"ios";
#ifdef DEBUG
        self.client_id = @"11111";
#else
        self.client_id = [APService registrationID];
#endif
    }
    return self;
}

+ (instancetype)param
{
    return [[self alloc] init];
}

@end
